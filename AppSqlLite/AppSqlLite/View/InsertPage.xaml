<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppSqlLite.View.InsertPage">

  <ContentPage.Content>
    <TableView Intent="Form">
      <TableRoot>
        <TableSection Title="Öğrenci Ekle">
          <EntryCell Label="Adınız" x:Name="txtName"></EntryCell>
          <EntryCell Label="Soyadınız" x:Name="txtSurname"></EntryCell>
          <ViewCell>
            <Button Text="Ekle" Clicked="Insert_OnClicked"></Button>
          </ViewCell>
        </TableSection>
      </TableRoot>
    </TableView>
  </ContentPage.Content>
  
</ContentPage>